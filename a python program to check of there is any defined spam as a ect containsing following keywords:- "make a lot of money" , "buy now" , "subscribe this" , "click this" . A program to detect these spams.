spam_phrases = [
    "make a lot of money",
    "buy now",
    "subscribe this",
    "click this"
]

user_input = input ("write the text if there is any spam than it will detect and tell you: ").lower()

found_spam = False

for phase in spam_phrases:
  if phase in user_input:
    found_spam = True
    break

if found_spam:
  print("its a spam messages")
else:
  print("its not a spam messages")
